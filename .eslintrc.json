{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "class-property"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "semi": "off",
    "react/prefer-stateless-function": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": "off"
  }
}
